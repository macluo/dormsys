<h1>Listing housing_requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @housing_requests.each do |housing_request| %>
      <tr>
        <td><%= link_to 'Show', housing_request %></td>
        <td><%= link_to 'Edit', edit_housing_request_path(housing_request) %></td>
        <td><%= link_to 'Destroy', housing_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Housing request', new_housing_request_path %>
