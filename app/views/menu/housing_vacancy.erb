<h1>List of housing and its vacancy</h1>

<table>
  <thead>
  <tr>
    <th colspan="6">Buildings and apartments</th>
    <th colspan="3">Vacancy</th>
  </tr>
  </thead>

  <tbody>
  <% @the_list.each do |i| %>
      <tr>
        <th colspan="6"><%= i[:unit_no] %></th>
        <th colspan="3"><%= i[:vacancy] %></th>
        <td><%#= link_to 'Show', parking_request %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<% if session[:user] == 'student' %>
   <%= link_to 'Back', menu_student_url %>
<% elsif session[:user] == 'staff' %>
   <%= link_to 'Back', menu_staff_url %>
<% end %>
